{
  "root": true,
  "extends": [
    "@react-native"
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "no-console": "error",
    "no-unused-vars": "warn",
    "react/prop-types": "off",
    "react-native/no-inline-styles": "off",
    "react-hooks/exhaustive-deps": "off"
  }
}